{
  "name": "morpheus",
  "productName": "Morpheus",
  "version": "0.0.2",
  "icon": "build/mtn.icns",
  "description": "Sovereign AI for the people",
  "main": "main.js",
  "homepage": "https://www.morepheusai.com",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "package-mac64": "electron-builder --mac --x64",
    "package-linux": "electron-builder --linux"
  },
  "keywords": [
    "Local",
    "Sovereign",
    "AI"
  ],
  "author": "Morpheus",
  "license": "Unlicense",
  "devDependencies": {
    "electron": "^27.0.2",
    "electron-builder": "^24.6.4"
  },
  "build": {
    "appId": "org.morpheus.client",
    "files": "![build_scripts{*/}",
    "icon": "build/mtn.icns",
    "forceCodeSigning": true,
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "mac": {
      "category": "public.app-category.productivity",
      "target": [
        "dmg",
        "zip"
      ]
    },
    "linux": {
      "compression": "normal",
      "target": "AppImage",
      "category": "Utility",
      "synopsis": "Morpheus is AI for the people",
      "description": "Decentralized AI for the people",
      "maintainer": "Morpheus <morepheusai@proton.me>",
      "vendor": "Morpheus",
      "executableName": "morpheus",
      "desktop": {
        "Name": "Morpheus",
        "Icon": "build/mtn.png"
      }
    }
  }
}